{
  "compilerOptions": {
    "plugins": [
      {
        "name": "typescript-tslint-plugin",
      }
    ],
    "lib": ["ES2021"],
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "CommonJS",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
  },
  "exclude": ["node_modules"]
}
